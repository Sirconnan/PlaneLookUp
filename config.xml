<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.smb.univ.etu.marietm.PlaneLookUp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Plane Look Up</name>
    <description>Application pour localiser les avions</description>
    <author>Medrick Mariet</author>
    <content src="index.html"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <icon src="res/mipmap-mdpi/ic_launcher.png" density="mdpi"/>
    <icon src="res/mipmap-hdpi/ic_launcher.png" density="hdpi"/>
    <icon src="res/mipmap-xhdpi/ic_launcher.png" density="xhdpi"/>
    <icon src="res/mipmap-xxhdpi/ic_launcher.png" density="xxhdpi"/>
    <icon src="res/mipmap-xxxhdpi/ic_launcher.png" density="xxxhdpi"/>
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="res/windows/splashscreen.png" />
    <preference name="SplashScreenBackgroundColor" value="#585353"/>
    <preference name="SplashScreenDelay" value="2000" />
</widget>
